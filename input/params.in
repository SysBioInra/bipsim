################################################################################
# INITIAL TIME 
# Time at the beginning of simulation. Format is  
#   INITIAL_TIME <real value>
################################################################################

INITIAL_TIME 0

################################################################################
# FINAL TIME 
# Time at which simulation should stop. Format is  
#   FINAL_TIME <real value>
################################################################################

FINAL_TIME 1000

################################################################################
# INPUT FILES 
# Write path to input files relative to binary. Any # number of files can be
# specified. Format is 
#   INPUT_FILES path/to/file1 path/to/file2 etc.
################################################################################

INPUT_FILES ../input/dna_info.in ../input/dna.in ../input/rna_info.in ../input/rnas.in ../input/protein_info.in ../input/proteins.in ../input/transcription.in ../input/metabolites.in ../input/translation.in

################################################################################
# OUTPUT FILE
# Write path to output file relative to binary. Format is 
#   OUTPUT_FILE path/to/output/file
################################################################################

OUTPUT_FILE ../output/output.txt

################################################################################
# OUTPUT STEP
# Frequency at which concentrations should be logged, in number of reactions.
# Format is
#   OUTPUT_STEP <integer value>
################################################################################

OUTPUT_STEP 100000

################################################################################
# OUTPUT ENTITIES
# Entities which should be logged in the output file. Format is
#   OUTPUT_ENTITIES name1 name2 etc.
################################################################################

OUTPUT_ENTITIES protein rna GTP CTP ATP UTP 50S
